{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Aboard API"
  },
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/api/healthcheck": {
      "get": {
        "responses": {
          "200": {
            "description": "Health check",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  },
                  "required": ["status"]
                }
              }
            }
          }
        }
      }
    },
    "/api/openapi": {
      "get": {
        "responses": {
          "200": {
            "description": "OpenAPI Specification JSON",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "openapi": {
                      "type": "string"
                    },
                    "info": {
                      "type": "object",
                      "properties": {
                        "title": {
                          "type": "string"
                        },
                        "version": {
                          "type": "string"
                        }
                      },
                      "required": ["title", "version"]
                    }
                  },
                  "required": ["openapi", "info"],
                  "additionalProperties": {
                    "nullable": true
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
